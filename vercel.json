{
  "rewrites": [
    {
      "source": "/projects/:folder/:file*",
      "destination": "/projects/:folder/:file*"
    },
    {
      "source": "/projects/:folder",
      "destination": "/projects/:folder/index.html"
    },
    {
      "source": "/((?!projects/).*)",
      "destination": "/index.html"
    }
  ],
  "cleanUrls": true
}