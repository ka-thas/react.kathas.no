{
  "rewrites": [
    {
      "source": "/projects/:path*",
      "destination": "/projects/:path*"
    },
    {
      "source": "/projects/:folder/",
      "destination": "/projects/:folder/index.html"
    },
    {
      "source": "/((?!projects/).*)",
      "destination": "/index.html"
    }
  ],
  "cleanUrls": true
}
